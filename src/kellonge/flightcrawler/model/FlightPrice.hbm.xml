<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="kellonge.flightcrawler.model">
	<class name="FlightPrice" table="FlightPrice">
		<id name="ID" column="ID" />
		<property name="FlightInfoID"  type="int" /> 
		<property name="Price"  type="big_decimal"/> 
		<property name="Cabin1"  type="string" /> 
		<property name="Cabin2"  type="string" /> 
		<property name="Cabin3"  type="string" /> 
		<property name="Remark"  type="string" /> 
	</class>
</hibernate-mapping>